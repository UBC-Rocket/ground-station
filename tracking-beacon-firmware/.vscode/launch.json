{
    "version": "0.2.0",
    "configurations": [
        {
            "name": "tracking-beacon - Debug",
            "cwd": "${workspaceFolder}",
            "executable": "${workspaceFolder}/build/Debug/tracking-beacon-firmware.elf",
            "request": "launch",
            "type": "cortex-debug",
            "servertype": "openocd",
            "configFiles": [
                "interface/stlink-dap.cfg",
                "target/stm32f4x.cfg"
            ],
            "svdFile": "${workspaceFolder}/platform/svd/STM32F446.svd",
            "searchDir": [
                "${env:STM32_OPENOCD_SCRIPTS_PATH}"
            ],
            "runToEntryPoint": "main",
            "showDevDebugOutput": "raw"
        },
        {
            "name": "tracking-beacon - Release",
            "cwd": "${workspaceFolder}",
            "executable": "${config:cmake.buildDirectory}/Release/tracking-beacon-firmware.elf",
            "request": "launch",
            "type": "cortex-debug",
            "servertype": "openocd",
            "configFiles": [
                "interface/stlink-dap.cfg",
                "target/stm32f4x.cfg"
            ],
            "svdFile": "${workspaceFolder}/platform/svd/STM32F446.svd",
            "searchDir": [
                "${env:STM32_OPENOCD_SCRIPTS_PATH}"
            ],
            "runToEntryPoint": "main",
            "showDevDebugOutput": "raw"
        }
    ]
}